{
    "name": "zicht/page-bundle",
    "description": "Zicht PageBundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "sonata-project/admin-bundle": "^4.21.0",
        "symfony/config": "^5.4 || ^6",
        "symfony/console": "^5.4 || ^6",
        "symfony/security-core": "^5.4 || ^6",
        "twig/twig": "^2.7 || ^3",
        "zicht/admin-bundle": "^8 || ^9.x-dev",
        "zicht/framework-extra-bundle": "^10 || ^11.x-dev",
        "zicht/menu-bundle": "^6 || ^7.x-dev",
        "zicht/url-bundle": "^7 || ^8.x-dev",
        "zicht/util": "^2"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3",
        "phpunit/phpunit": "^9.6",
        "symfony/form": "^5.4",
        "vimeo/psalm": "^5"
    },
    "autoload": {
        "psr-4": {
            "Zicht\\Bundle\\PageBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ZichtTest\\Bundle\\PageBundle\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "scripts": {
        "analysis": "psalm",
        "lint": "php-cs-fixer fix --dry-run --diff -vvv",
        "lint-fix": "php-cs-fixer fix",
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-feature-symfony-6": "9.x-dev"
        }
    }
}

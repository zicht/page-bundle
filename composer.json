{
    "name": "zicht/page-bundle",
    "description": "Zicht PageBundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": "^7.2 || ^8.0",
        "sonata-project/admin-bundle": "^3.1",
        "symfony/config": "^4.4",
        "symfony/console": "^4.4",
        "symfony/security-core": "^4.4",
        "symfony/templating": "^4.4",
        "zicht/admin-bundle": "^6.1",
        "zicht/framework-extra-bundle": "^9",
        "zicht/menu-bundle": "^4 || ^5",
        "zicht/url-bundle": "^5",
        "zicht/util": "^1.7 || ^2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8"
    },
    "autoload": {
        "psr-4": {
            "Zicht\\Bundle\\PageBundle\\": [
                "src/"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ZichtTest\\Bundle\\PageBundle\\": [
                "tests/"
            ]
        }
    },
    "minimum-stability": "stable",
    "scripts": {
        "test": [
            "phpunit -c phpunit.xml.dist"
        ]
    }
}
